      "(?i)technologies"
    ],
    "education": [
      "(?i)education",
      "(?i)academic\\s+background",
      "(?i)qualifications"
    ],
    "projects": [
      "(?i)projects?",
      "(?i)personal\\s+projects?",
      "(?i)portfolio"
    ]
